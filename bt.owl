bt:CloseGripper a bt:Node ;
    bt:description "Abstract action to close the gripper"^^xsd:string ;
    bt:mapsTo [ a bt:Node ;
            bt:type "GripperCommand" ;
            bt:withParams [ bt:position 0e+00 ] ],
        [ a bt:Node ;
            bt:type "YaskawaGripperCommand" ;
            bt:withParams [ bt:position 0e+00 ] ] .

bt:GetCurrentIKFramePose a bt:Node ;
    bt:description "Query the current PoseStamped of the specified IK-frame (default \"tool0\") for feedback control or as a starting point for relative planning."^^xsd:string ;
    bt:hasPort bt:GetCurrentIKFramePose_error_code_id,
        bt:GetCurrentIKFramePose_group_name,
        bt:GetCurrentIKFramePose_ik_frame,
        bt:GetCurrentIKFramePose_pose,
        bt:GetCurrentIKFramePose_server_name,
        bt:GetCurrentIKFramePose_server_timeout .

bt:OpenGripper a bt:Node ;
    bt:description "Abstract action to open the gripper"^^xsd:string ;
    bt:mapsTo [ a bt:Node ;
            bt:type "GripperCommand" ;
            bt:withParams [ bt:position 1e+00 ] ],
        [ a bt:Node ;
            bt:type "YaskawaGripperCommand" ;
            bt:withParams [ bt:position 1e+00 ] ] .

bt:PrintROSMsg a bt:Node ;
    bt:description "Print ROS messages (String, RobotTrajectory, PoseStamped, Pose) to console for debugging or introspection during task execution."^^xsd:string ;
    bt:hasPort bt:PrintROSMsg_msg_pose,
        bt:PrintROSMsg_msg_posestamped,
        bt:PrintROSMsg_msg_string,
        bt:PrintROSMsg_msg_trajectory .

bt:SetPathConstraints a bt:Node ;
    bt:controlFlowNext bt:ComputePathToPoint,
        bt:ComputePathToPose,
        bt:ComputePathToState ;
    bt:description "Generate a Constraints message on-the-fly from a semicolon-separated list of joint limits (e.g. \"joint1:-1.57,1.57;joint2:-0.8,0.8\")."^^xsd:string ;
    bt:fault_rate 1e-02 ;
    bt:hasPort bt:SetPathConstraints_joint_constraints,
        bt:SetPathConstraints_path_constraints ;
    bt:probability 1e+00 .

